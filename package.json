{
  "name": "electron-sveltekit",
  "description": "Electron, SvelteKit, Bootstrap 5 and FontAwesome",
  "author": "anonymous",
  "version": "1.0.0",
  "main": "src/electron.cjs",
  "private": true,
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "electron": "concurrently --kill-others \"svelte-kit dev\" \"electron src/electron.cjs\"",
    "build": "svelte-kit build",
    "dev:package": "npm run build && electron-builder --config electron-builder.config.json --dir",
    "package": "npm run build && electron-builder --config electron-builder.config.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.24",
    "@sveltejs/kit": "next",
    "@types/cron": "^2.0.0",
    "@types/lodash": "^4.14.182",
    "@types/tmi.js": "^1.8.1",
    "concurrently": "^7.2.1",
    "electron": "^12.0.2",
    "electron-builder": "^23.0.3",
    "electron-reloader": "^1.2.3",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.8.1",
    "vite": "^2.1.0",
    "vite-plugin-windicss": "^1.8.4"
  },
  "type": "module",
  "dependencies": {
    "axios": "^0.27.2",
    "case": "^1.6.3",
    "cheerio": "^1.0.0-rc.11",
    "cron": "^2.0.0",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "firebase": "^9.8.4",
    "firebase-admin": "^10.3.0",
    "firefox": "^0.0.1",
    "lodash": "^4.17.21",
    "puppeteer": "^14.3.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "queue-promise": "^2.2.1",
    "tmi.js": "^1.8.5",
    "typescript": "^4.7.4"
  }
}
